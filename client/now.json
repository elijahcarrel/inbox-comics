{
  "version": 2,
  "name": "app",
  "alias": ["app.inboxcomics.now.sh"],
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/subscribe.php?modifysettings=yes&email=(.*)", "status": 301, "headers": { "Location": "/user?email=$1" }},
    { "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+", "headers": { "cache-control": "immutable" } }
  ]
}
