{
  "version": 2,
  "name": "api",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node"
    }
  ],
  "env": {
    "mongodb_url": "@mongodb_url",
    "elasticemail_api_key": "@elasticemail_api_key",
    "domain": "https://www.inboxcomics.com",
    "graphql_http_endpoint": "https://api.inboxcomics.com/graphql"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/src/index.ts" }
  ]
}
