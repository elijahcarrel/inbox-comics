{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "api/src/index.ts", "use": "@now/node" }
  ],
  "env": {
    "mongodb_url": "@mongodb_url",
    "elasticemail_api_key": "@elasticemail_api_key",
    "domain": "https://www.inboxcomics.com"
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/src/index.ts" }
  ]
}
